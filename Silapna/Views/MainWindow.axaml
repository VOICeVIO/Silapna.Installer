<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Silapna.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Silapna.Views"
        Height="350" Width="700"
        mc:Ignorable="d" d:DesignWidth="700" d:DesignHeight="350"
        x:Class="Silapna.Views.MainWindow"
        Icon="/Assets/VP.ico"
        Title="Silapna"
        >
    <views:MainView HorizontalAlignment="Stretch" VerticalAlignment="Stretch" DataContext="{Binding $parent[views:MainWindow].DataContext}" />
</Window>
